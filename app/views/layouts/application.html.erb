	<!DOCTYPE html>
  <html>
  <head>
    <title>Dreamart</title>
    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <script type="text/javascript">if($(window).width()>1024){document.write("<"+"script src='jquery.preloader.js'></"+"script>");}  </script>
    <script>    
     jQuery(window).load(function() { 
       $x = $(window).width();    
       if($x > 1024)
       {     
        jQuery("#content .row").preloader();    }      

        jQuery('.spinner').animate({'opacity':0},1000,'easeOutCubic',function (){jQuery(this).css('display','none')}); 
      }); 

   </script>
 </head>
 <body>
  <div class="spinner"></div>
  <!--============================== header =================================-->
  <header>
    <div class="container clearfix">
      <div class="row">
        <div class="span12">
          <div class="navbar navbar_">
            <div class="container">
              <h1 class="brand brand"><!--<img alt="" src="logo.gif"> -->DreamART</h1>
              <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse_">Menu <span class="icon-bar"></span> </a>
              <div class="nav-collapse nav-collapse_  collapse">
                <ul class="nav sf-menu">
                  <li>

                    <li><a href="/artworks">Art Gallery</a></li>
                    <li class="nav navbar-nav"> <a href="/artworks/new">New Artwork</a></li>
                    <% if artist_signed_in? %>
                    
                    <li><%= link_to 'Edit profile', edit_artist_registration_path, :class => 'navbar-link' %></li> 
                    <li><%= link_to "Logout", destroy_artist_session_path, method: :delete, :class => 'navbar-link'  %></li>
                    <% else %>
                    <li><%= link_to "Sign up", new_artist_registration_path, :class => 'navbar-link'  %></li>
                    <li><%= link_to "Login", new_artist_session_path, :class => 'navbar-link'  %></li>
                    <% end %>
                    

                    <li class="navbar-text" style="margin-top: 20px; margin-left: 100px; margin-right: -200px;">
                      <% if artist_signed_in? %>
                      Logged in as <strong><%= current_artist.email %></strong>.
                      <% end %>
                    </li>
               <!-- <li><a href="index.html">About</a></li>
                <li class="sub-menu active"><a href="index-1.html">Services</a>
                      <ul>
                    <li><a href="#">Dolore </a></li>
                    <li><a href="#">Consecte</a></li>
                    <li><a href="#">Conseq</a></li>
                  </ul>
                    </li>
               <!-- <li><a href="index-2.html">Folio</a></li>
                <li><a href="index-3.html">Blog</a></li>
                <li><a href="index-4.html">Contact</a></li>-->
              </ul>
            </div>
          </div>
          

       </div>
     </div>
   </div>
 </div>
</header>
<div class="bg-content"> 

  <!--============================== content =================================-->

  <div id="content"><div class="ic">More Website Templates @ TemplateMonster.com. November19, 2012!</div>
  <div class="container">
    <div class="row">
      <article class="span12">
        <h3></h3>
      </article>
      <div class="clear"></div>
      <%= yield %>
    </div>
  </div>
</div>
</div>

<!--============================== footer =================================-->
<footer>
  <div class="container clearfix">
    <ul class="list-social pull-right">
      <li><a class="icon-1" href="https://www.facebook.com/"></a></li>
      <li><a class="icon-2" href="#"></a></li>
      <li><a class="icon-3" href="#"></a></li>
      <li><a class="icon-4" href="https://plus.google.com/u/0/"></a></li>
    </ul>
    <div class="privacy pull-left">Website Template designed by <a href="#" target="_blank" rel="nofollow">Derek Owusu-Frimpong</a> </div>
  </div>
</footer>
</body>
</html>